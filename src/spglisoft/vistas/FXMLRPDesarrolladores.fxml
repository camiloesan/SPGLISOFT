<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="600.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="spglisoft.controladores.FXMLRPDesarrolladoresController">
   <children>
      <HBox prefHeight="600.0" prefWidth="1000.0">
         <children>
            <VBox alignment="CENTER" prefHeight="200.0" prefWidth="200.0" spacing="25.0" style="-fx-background-color: #003466;" HBox.hgrow="NEVER">
               <children>
                  <Label layoutX="78.0" layoutY="302.0" text="Actividades" textFill="WHITE">
                     <cursor>
                        <Cursor fx:constant="HAND" />
                     </cursor>
                  </Label>
                  <Separator layoutX="70.0" layoutY="253.0" maxWidth="80.0" opacity="0.6" prefWidth="200.0" />
                  <Label layoutX="49.0" layoutY="319.0" onMouseClicked="#btnCambios" text="Cambios" textFill="WHITE">
                     <cursor>
                        <Cursor fx:constant="HAND" />
                     </cursor>
                  </Label>
                  <Separator layoutX="70.0" layoutY="302.0" maxWidth="80.0" opacity="0.6" prefWidth="200.0" />
                  <Label layoutX="85.0" layoutY="310.0" text="Defectos" textFill="WHITE">
                     <cursor>
                        <Cursor fx:constant="HAND" />
                     </cursor>
                  </Label>
                  <Separator layoutX="70.0" layoutY="350.0" maxWidth="80.0" opacity="0.6" prefWidth="200.0" />
                  <Label layoutX="67.0" layoutY="327.0" onMouseClicked="#btnDesarrolladores" text="Desarrolladores" textFill="WHITE" underline="true">
                     <cursor>
                        <Cursor fx:constant="HAND" />
                     </cursor>
                  </Label>
                  <Separator layoutX="70.0" layoutY="399.0" maxWidth="80.0" opacity="0.6" prefWidth="200.0" />
                  <Label layoutX="67.0" layoutY="373.0" text="Información del Proyecto" textFill="WHITE">
                     <cursor>
                        <Cursor fx:constant="HAND" />
                     </cursor>
                  </Label>
                  <Separator layoutX="70.0" layoutY="415.0" maxWidth="80.0" opacity="0.6" prefWidth="200.0" />
                  <HBox alignment="BOTTOM_LEFT" prefHeight="70.0" prefWidth="200.0">
                     <children>
                        <Label onMouseClicked="#btnRegresar" text="Regresar" textFill="WHITE" underline="true">
                           <cursor>
                              <Cursor fx:constant="HAND" />
                           </cursor>
                           <HBox.margin>
                              <Insets left="20.0" />
                           </HBox.margin>
                        </Label>
                     </children>
                  </HBox>
               </children>
            </VBox>
            <VBox alignment="TOP_CENTER" prefHeight="200.0" prefWidth="100.0" spacing="25.0" HBox.hgrow="ALWAYS">
               <children>
                  <Label text="Desarrolladores">
                     <font>
                        <Font size="18.0" />
                     </font>
                     <VBox.margin>
                        <Insets bottom="30.0" top="30.0" />
                     </VBox.margin>
                  </Label>
                  <HBox>
                     <VBox.margin>
                        <Insets left="25.0" right="25.0" />
                     </VBox.margin>
                  </HBox>
                  <TableView fx:id="tvActividades" prefHeight="200.0" prefWidth="200.0" VBox.vgrow="ALWAYS">
                     <columnResizePolicy>
                        <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                     </columnResizePolicy>
                     <columns>
                        <TableColumn fx:id="colTitulo" prefWidth="75.0" text="Nombre" />
                        <TableColumn fx:id="colFechaInicio" prefWidth="75.0" text="Apellido Paterno" />
                        <TableColumn fx:id="colFechaFin" prefWidth="75.0" text="Apellido Materno" />
                        <TableColumn fx:id="colEstado" prefWidth="75.0" text="Matricula" />
                        <TableColumn prefWidth="75.0" text="Semestre" />
                     </columns>
                     <VBox.margin>
                        <Insets left="25.0" right="25.0" />
                     </VBox.margin>
                  </TableView>
                  <HBox alignment="CENTER_RIGHT" spacing="15.0">
                     <children>
                        <Button fx:id="btnEliminarActividad" mnemonicParsing="false" onAction="#btnEliminarDesarrollador" text="Eliminar Desarrollador" />
                        <Button fx:id="btnAsignarActividad" mnemonicParsing="false" onAction="#btnAnadirDesarrollador" text="Añadir Desarrollador" />
                     </children>
                     <padding>
                        <Insets right="25.0" />
                     </padding>
                     <VBox.margin>
                        <Insets bottom="50.0" />
                     </VBox.margin>
                  </HBox>
               </children>
            </VBox>
         </children>
      </HBox>
   </children>
</AnchorPane>
